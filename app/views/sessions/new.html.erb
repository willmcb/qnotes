<h1 class="logo">Qnotes</h1>

<h3 class="tag_line">A simple way to organise your markdown code notes</h3>

<%= form_for :login do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email, autofocus: true %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.submit "Log in" %>
  </div>
<% end %>

